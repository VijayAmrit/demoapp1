{
    "_version": "1.32.0",
    "sap.app": {
        "id": "za.spar.customer.demoapp1",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "dataSources": {
            "mainService": {
                "uri": "/V3/Northwind/Northwind.svc/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "za-spar-customer-demoapp1-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "Customer",
                    "action": "display",
                    "title": "{{flpTitle}}",
                    "subTitle": "{{flpSubtitle}}",
                    "icon": ""
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.97.0",
            "libs": {
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "za.spar.customer.demoapp1.i18n.i18n"
                }
            },
            "masterModel": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {}
            },
            "products": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {},
                "uri": "model/Products.json",
                "preload": true
            },
            "detailModel": {
                "type": "sap.ui.model.json.JSONModel",
                "preload": false,
                ""
                "settings": {}
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "za.spar.customer.demoapp1.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "master",
                    "pattern": "",
                    "target": "master"
                },
                {
                    "name": "detail",
                    "pattern": "detail/{customerPath}",
                    "target": "detail"
                }
            ],
            "targets": {
                "master": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "master",
                    "viewName": "master"
                },
                "detail": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "detail",
                    "viewName": "detail"
                }
            }
        },
        "rootView": {
            "viewName": "za.spar.customer.demoapp1.view.main",
            "type": "XML",
            "async": true,
            "id": "main"
        }
    }
}